alias ls='ls --color=auto'
alias dir='ls -l'
alias rm='rm -i'
alias less='less -i -M -R'
alias grep='grep --color=auto'
alias tiga='tig --all'
alias df='duf -only local'
alias du='du -hs'
alias dmesg='dmesg -T'
alias root='sudo -i'
alias dvgrab='dvgrab --autosplit --format dv2 --timestamp --size 2048 --opendml'
alias exifdate='exiftool -a -s -G1 -time:all'
